             xmlns:local="clr-namespace:MetaNet"
             xmlns:tbex="clr-namespace:TextBoxEx"
             StartupUri="Views\MainWindow.xaml">
    <Application.Resources>

        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>

                <ResourceDictionary Source="Styles.xaml"/>
                
